<Window x:Class="Dobbelaar.Views.Photo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Photo" Height="150" Width="200" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <ResourceDictionary>
            <ImageSource x:Key="Image1">pack://siteoforigin:,,,/Interface/Images/Image1.jpg</ImageSource>
            <ImageSource x:Key="Image2">pack://siteoforigin:,,,/Interface/Images/Image2.jpg</ImageSource>
            <ImageSource x:Key="Image3">pack://siteoforigin:,,,/Interface/Images/Image3.jpg</ImageSource>
            <ImageSource x:Key="Image4">pack://siteoforigin:,,,/Interface/Images/Image4.jpg</ImageSource>
            <ImageSource x:Key="Image5">pack://siteoforigin:,,,/Interface/Images/Image5.jpg</ImageSource>
            <ImageSource x:Key="Image6">pack://siteoforigin:,,,/Interface/Images/Image6.jpg</ImageSource>
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <Image x:Name="Image" Margin="60,25,60,30" Source="pack://siteoforigin:,,,/Interface/Images/Image1.jpg"/>
    </Grid>
</Window>
